{
    "all": true,
    "src": [
        "src"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/**/*.test.ts",
        "src/**/*.spec.ts",
        "examples/**/*",
        "dist/**/*",
        "node_modules/**/*"
    ],
    "reporter": [
        "text",
        "html",
        "lcov"
    ],
    "report-dir": "coverage",
    "check-coverage": false,
    "lines": 0,
    "functions": 0,
    "branches": 0,
    "statements": 0,
    "extension": [
        ".ts"
    ],
    "temp-directory": ".nyc_output"
}